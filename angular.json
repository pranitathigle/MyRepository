{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "RADUI-Library": {
            "projectType": "library",
            "root": "projects/radui-library",
            "sourceRoot": "projects/radui-library/src",
            "prefix": "lib",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-ng-packagr:build",
                    "options": {
                        "tsConfig": "projects/radui-library/tsconfig.lib.json",
                        "project": "projects/radui-library/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/radui-library/tsconfig.lib.prod.json"
                        }
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/radui-library/src/test.ts",
                        "tsConfig": "projects/radui-library/tsconfig.spec.json",
                        "karmaConfig": "projects/radui-library/karma.conf.js"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "projects/radui-library/tsconfig.lib.json",
                            "projects/radui-library/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        },
        "RADUI-Application": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "projects/RADUI-Application",
            "sourceRoot": "projects/RADUI-Application/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/RADUI-Application",
                        "index": "projects/RADUI-Application/src/index.html",
                        "main": "projects/RADUI-Application/src/main.ts",
                        "polyfills": "projects/RADUI-Application/src/polyfills.ts",
                        "tsConfig": "projects/RADUI-Application/tsconfig.app.json",
                        "aot": true,
                        "assets": [
                            "projects/RADUI-Application/src/favicon.ico",
                            "projects/RADUI-Application/src/assets"
                        ],
                        "styles": [{
                                "input": "node_modules/@angular/material/prebuilt-themes/indigo-pink.css"

                            },
                            "projects/RADUI-Application/src/styles.scss", "node_modules/aos/dist/aos.css", "node_modules/swiper/css/swiper.css"
                        ],
                        "scripts": ["node_modules/aos/dist/aos.js"]
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [{
                                "replace": "projects/RADUI-Application/src/environments/environment.ts",
                                "with": "projects/RADUI-Application/src/environments/environment.prod.ts"
                            }],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [{
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "RADUI-Application:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "RADUI-Application:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "RADUI-Application:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/RADUI-Application/src/test.ts",
                        "polyfills": "projects/RADUI-Application/src/polyfills.ts",
                        "tsConfig": "projects/RADUI-Application/tsconfig.spec.json",
                        "karmaConfig": "projects/RADUI-Application/karma.conf.js",
                        "assets": [
                            "projects/RADUI-Application/src/favicon.ico",
                            "projects/RADUI-Application/src/assets"
                        ],
                        "styles": [
                            "projects/RADUI-Application/src/styles.scss",
                            {
                                "input": "node_modules/@angular/material/prebuilt-themes/indigo-pink.css"
                            }
                        ],
                        "scripts": []
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "projects/RADUI-Application/tsconfig.app.json",
                            "projects/RADUI-Application/tsconfig.spec.json",
                            "projects/RADUI-Application/e2e/tsconfig.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "projects/RADUI-Application/e2e/protractor.conf.js",
                        "devServerTarget": "RADUI-Application:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "RADUI-Application:serve:production"
                        }
                    }
                }
            }
        }
    },
    "defaultProject": "RADUI-Library"
}
